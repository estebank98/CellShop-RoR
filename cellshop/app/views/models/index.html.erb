<p id="notice"><%= notice %></p>

<h1>Models</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Resolution</th>
      <th>Screen</th>
      <th>Weight</th>
      <th>Dimensions</th>
      <th>Batery</th>
      <th>Brand</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @models.each do |model| %>
      <tr>
        <td><%= model.name %></td>
        <td><%= model.resolution %></td>
        <td><%= model.screen %></td>
        <td><%= model.weight %></td>
        <td><%= model.dimensions %></td>
        <td><%= model.batery %></td>
        <td><%= model.brand %></td>
        <td><%= link_to 'Show', model %></td>
        <td><%= link_to 'Edit', edit_model_path(model) %></td>
        <td><%= link_to 'Destroy', model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Model', new_model_path %>
